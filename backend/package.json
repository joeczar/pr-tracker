{
  "name": "pr-tracker-backend",
  "version": "1.0.0",
  "description": "Backend API for PR Progress Tracker",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "start": "bun dist/index.js",
    "db:migrate": "bun src/db/migrate.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.0",
    "@octokit/rest": "^21.0.0",
    "cookie": "^1.0.2",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.1",
    "hono": "^4.6.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^11.1.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/cookie": "^1.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.5.0",
    "@types/uuid": "^10.0.0",
    "bun-types": "^1.1.0",
    "typescript": "^5.6.0"
  },
  "keywords": [
    "github",
    "pr-tracking",
    "hono",
    "typescript"
  ],
  "author": "Joe Czarnomorph",
  "license": "MIT"
}

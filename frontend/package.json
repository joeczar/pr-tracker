{
  "name": "pr-tracker-frontend",
  "version": "1.0.0",
  "description": "Frontend for PR Progress Tracker",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@radix-icons/vue": "^1.0.0",
    "@vueuse/core": "^11.3.0",
    "axios": "^1.7.0",
    "chart.js": "^4.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-vue-next": "^0.536.0",
    "pinia": "^2.2.0",
    "reka-ui": "^2.4.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.0",
    "vue-chartjs": "^5.3.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@tailwindcss/forms": "^0.5.9",
    "@types/node": "^22.5.0",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.28.0",
    "postcss": "^8.4.0",
    "shadcn-vue": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0",
    "vite": "^5.4.0",
    "vitest": "^2.1.0",
    "vue-tsc": "^2.1.0"
  },
  "keywords": [
    "vue",
    "typescript",
    "vite",
    "pr-tracking"
  ],
  "author": "Joe Czarnomorph",
  "license": "MIT"
